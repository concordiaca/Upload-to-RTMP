

from keras.applications.resnet50 import ResNet50
from keras.applications.resnet50 import preprocess_input, decode_predictions
import cv2
import numpy as np
import subprocess

print("[INFO] Loading network")
model = ResNet50(weights="imagenet")

cap = cv2.VideoCapture(0)



while (True):
    ret, original =cap.read(0)
    frame = cv2.resize(original, (224,224))
    image = cv2.cvtColor(frame, cv2.COLOR_BGR2RGB).astype(np.float32)
    image = image.reshape((1,) + image.shape)

    image = preprocess_input(image)
    preds = model.predict(image)
    label = decode_predictions(preds, top=1)[0]

    cv2.putText(original, "Label: {}".format(label), (10, 30), cv2.FONT_HERSHEY_SIMPLEX, 0.9, (0,255,0), 2)
    cv2.imshow("Classification", original)
    print('Predicted:', decode_predictions(preds, top=1)[0])

    strcmd = "/Usr/local/Cellar/ffmpeg/3.4.2/bin/ffmpeg -re -i video= -vcodec libx264 -f flv rtmp://172.30.154.193/rtmplive/room2"
    subprocess.call(strcmd, shell=True)

    if (cv2.waitKey(1) & 0xFF == ord('q')):\
        break
cap.release()
cv2.destroyAllWindows()



